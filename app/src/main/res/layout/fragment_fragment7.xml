<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.suhyun.gizi2.Fragment1">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        >

        <Button
            android:layout_weight="1"
            android:id="@+id/bt_tab6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:drawableTop="@drawable/bookmark_white"
            android:padding="5dp"
            android:background="@color/marina"
            android:text="처리 전 민원"
            android:textColor="#FFFFFF"
            />

        <Button
            android:layout_weight="1"
            android:id="@+id/bt_tab7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:drawableTop="@drawable/bookmark"
            android:background="@color/marina"
            android:padding="5dp"
            android:text="처리 후 민원"
            android:textColor="#FFFFFF"
            />
    </LinearLayout>
</FrameLayout>